<Generic optr='ExtDefList'>
    <Ext optr='extdef_func'>
        <FunDef optr='funhead_def'>
            <Type val='int'/>
            <Generic optr='func_dec'>
                <ID val='main'/>
            </Generic>
        </FunDef>
        <CompStmt optr='compst'>
            <LocalDec optr='dec'>
                <Type val='int'/>
                <Generic optr='init_assign'>
                    <ID val='a'/>
                    <NUM val='1'/>
                </Generic>
            </LocalDec>
            <LocalDec optr='dec'>
                <Type val='int'/>
                <Generic optr='init_assign'>
                    <ID val='b'/>
                    <NUM val='2'/>
                </Generic>
            </LocalDec>
            <LocalDec optr='dec'>
                <Type val='int'/>
                <Generic optr='init_assign'>
                    <ID val='c'/>
                    <NUM val='3'/>
                </Generic>
            </LocalDec>
            <FlowCtrl optr='while'>
                <Calc optr='<'>
                    <Identifier val='a'/>
                    <Identifier val='b'/>
                </Calc>
                <CompStmt optr='compst'>
                    <Calc optr='='>
                        <Identifier val='c'/>
                        <Calc optr='+'>
                            <Identifier val='c'/>
                            <NUM val='3'/>
                        </Calc>
                    </Calc>
                </CompStmt>
            </FlowCtrl>
            <Stmt optr='return'>
                <NUM val='0'/>
            </Stmt>
        </CompStmt>
    </Ext>
</Generic>